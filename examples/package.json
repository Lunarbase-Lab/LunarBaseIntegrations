{
  "name": "dex-integration-examples",
  "version": "1.0.0",
  "description": "Integration examples for DEX protocol on Base network",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "example:pools": "npx tsx scripts/01-discover-pools.ts",
    "example:fees": "npx tsx scripts/02-extract-pool-fees.ts",
    "example:swap": "npx tsx scripts/03-simulate-swap.ts",
    "example:dynamic-fee": "npx tsx scripts/04-calculate-dynamic-fee.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "dex",
    "amm",
    "base",
    "swap",
    "liquidity"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "dotenv": "^16.4.5",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  }
}
